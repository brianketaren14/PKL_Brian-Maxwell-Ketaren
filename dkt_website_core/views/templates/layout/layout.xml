<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="dkt_website_core.homepage" name="Karo Homepage" inherit_id="website.homepage">
        <xpath expr="//div[@id='wrap']" position="replace">
            <div id="wrap" class="o_website_wrap position-relative overflow-auto">
                <main class="main-content position-relative">
                    <div class="banner-section">
                        <t t-call="dkt_website_core.banner_carousel"/>
                    </div>
                    
                    <div class="services-section">
                        <t t-call="dkt_website_core.government_services_section"/>
                    </div>
                    
                    <!-- <div class="events-section">
                        <t t-call="dkt_website_core.event_section"/>
                    </div> -->
                    
<!--                    <div class="news-section">-->
<!--                        <t t-call="dkt_website_core.news_section"/>-->
<!--                    </div>-->
                    
                    <div class="stats-section">
                        <t t-call="dkt_website_core.visitor_stats_template"/>
                    </div>
                    
                    <div class="map-section">
                        <t t-call="dkt_website_core.map_section"/>
                    </div>
                </main>
            </div>
        </xpath>
    </template>
</odoo> 